// jsconfig.json
{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "@components/*": ["src/components/*"],
        "@assets/*": ["/src/assets/*"],
        "@utils/*": ["src/utils/*"],
        "@services/*": ["src/services/*"],
        "@store/*": ["src/store/*"],
        "@hooks/*": ["src/hooks/*"],
        "@icons*": ["/src/assets/icons/*"],
        "@constants*": ["/src/constants"],
        "@context*": ["/src/context"],
        "@pages*":["/src/pages/*"],
        "@routes*": ["/src/routes/*"],
        "@theme*": ["/src/theme/*"]
      }
    }
  }